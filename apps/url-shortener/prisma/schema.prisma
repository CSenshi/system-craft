generator client {
  provider        = "prisma-client"
  output          = "../src/prisma/generated"
  engineType      = "client"
  moduleFormat    = "cjs"
  previewFeatures = ["typedSql"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShortendUrls {
  id  BigInt @id
  url String

  @@map("shortend_urls")
}
