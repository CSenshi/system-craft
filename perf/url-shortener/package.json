{
  "name": "@perf/url-shortener",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "setup": {
        "executor": "nx:run-commands",
        "options": {
          "command": "docker run --rm -it -e GOOS=darwin -u \"$(id -u):$(id -g)\" -v \"${PWD}:/xk6\" grafana/xk6 build v0.45.1 --with github.com/grafana/xk6-dashboard@latest"
        }
      },
      "bench": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./k6 run ./perf/url-shortener/src/bench.js --out dashboard"
        }
      }
    }
  }
}
